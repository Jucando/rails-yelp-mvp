<h1>All Restaurants</h1>
<ul>
<% @restaurants.each do |restaurant| %>
  <li><div class="card">
    <h2><%= link_to restaurant.name, restaurant_path(restaurant.id) %></h2>
    <p><%= restaurant.address %></p>
    <p><%= restaurant.phone_number %></p>
    <p><%= restaurant.category %></p>
  </div></li>
<% end %>
</ul>
